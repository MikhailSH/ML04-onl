    "#функция меняет первый и последний символ в полученом списке\n",
    "mylist = [10, 2, 3, 4, 5, 6, 7, 8, 9, 1]\n",
    "print(mylist)\n",
    "def change(mylist):\n",
    "    num = mylist[0]\n",
    "    mylist[0] = mylist[len(mylist)-1]\n",
    "    mylist[len(mylist)-1] = num\n",
    "    print(mylist)\n",
    "change(mylist)    \n",
